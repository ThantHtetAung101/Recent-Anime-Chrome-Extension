{
    "manifest_version": 2,
    "name": "ReAnime",
    "version": "1.0",
    "content_security_policy": "script-src 'self' https://cdn.lordicon.com/ritcuqlt.js; object-src 'self'",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
    "icons": {                   
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"  
    }
}
